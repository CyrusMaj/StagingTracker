@model IEnumerable<StagingTracker.Data.QualityControl>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.InspectionDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InspectionResult)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InspectorName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Receiving)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.InspectionDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InspectionResult)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InspectorName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Receiving.ReceivingId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Qcid">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Qcid">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Qcid">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
